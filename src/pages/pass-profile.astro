---
import Layout from '../layouts/BaseLayout.astro';
import SectionDivider from '../components/SectionDivider.astro';
import Breadcrumb from '../components/Breadcrumb.astro';
---

<Layout title="PASS Profile™ — Communication Assessment" description="Professional communication assessment: 60 workplace scenarios, 40 traits, personalized PDF report. $49.99 individual or $59.99 with Voice-Brand Alignment." ogImage="/images/og/og-pass-profile.jpg">
  <Fragment slot="head">
    <script type="application/ld+json" set:html={JSON.stringify({
      "@context": "https://schema.org",
      "@type": "Product",
      "name": "PASS Profile Communication Assessment",
      "description": "Professional communication assessment measuring 40 traits across 6 domains via 60 workplace scenarios. Delivers a personalized 35-48 page PDF report revealing your unique communication fingerprint.",
      "image": "https://tedsvoiceacademy.com/images/pass-profile-logo.webp",
      "brand": { "@type": "Brand", "name": "PASS Profile" },
      "creator": { "@type": "Person", "name": "Ted Chamberlain", "url": "https://tedsvoiceacademy.com/about" },
      "offers": {
        "@type": "Offer",
        "url": "https://pass.tedsvoiceacademy.com",
        "priceCurrency": "USD",
        "price": "49.99",
        "availability": "https://schema.org/InStock",
        "priceValidUntil": "2026-12-31",
        "seller": { "@type": "Organization", "name": "Ted's Voice Academy", "url": "https://tedsvoiceacademy.com" }
      },
      "category": "Professional Development"
    })} />
    <script type="application/ld+json" set:html={JSON.stringify({
      "@context": "https://schema.org",
      "@type": "Product",
      "name": "PASS Profile + Voice-Brand Alignment Bundle",
      "description": "Complete communication intelligence bundle: PASS Profile assessment (35-48 page report) plus Voice-Brand Alignment (8-10 page role-specific report) with personal communication brand statement, strategic playbook, and 90-day integration plan.",
      "image": "https://tedsvoiceacademy.com/images/pass-voice-brand-logo.webp",
      "brand": { "@type": "Brand", "name": "PASS Profile" },
      "creator": { "@type": "Person", "name": "Ted Chamberlain", "url": "https://tedsvoiceacademy.com/about" },
      "offers": {
        "@type": "Offer",
        "url": "https://pass.tedsvoiceacademy.com",
        "priceCurrency": "USD",
        "price": "59.99",
        "availability": "https://schema.org/InStock",
        "priceValidUntil": "2026-12-31",
        "seller": { "@type": "Organization", "name": "Ted's Voice Academy", "url": "https://tedsvoiceacademy.com" }
      },
      "category": "Professional Development"
    })} />
    <script type="application/ld+json" set:html={JSON.stringify({
      "@context": "https://schema.org",
      "@type": "Product",
      "name": "Team Dynamics Alignment Report",
      "description": "Team communication analysis for groups of 2-12 members. Reveals pair dynamics, six communication roles, collective strengths, and team-specific scenarios. Requires completed PASS Profile for each team member.",
      "image": "https://tedsvoiceacademy.com/images/team-dynamics-logo.webp",
      "brand": { "@type": "Brand", "name": "PASS Profile" },
      "creator": { "@type": "Person", "name": "Ted Chamberlain", "url": "https://tedsvoiceacademy.com/about" },
      "offers": {
        "@type": "AggregateOffer",
        "url": "https://pass.tedsvoiceacademy.com",
        "priceCurrency": "USD",
        "lowPrice": "39.99",
        "highPrice": "119.99",
        "offerCount": 3,
        "availability": "https://schema.org/InStock",
        "seller": { "@type": "Organization", "name": "Ted's Voice Academy", "url": "https://tedsvoiceacademy.com" }
      },
      "category": "Professional Development"
    })} />
    <script type="application/ld+json" set:html={JSON.stringify({
      "@context": "https://schema.org",
      "@type": "BreadcrumbList",
      "itemListElement": [
        { "@type": "ListItem", "position": 1, "name": "Home", "item": "https://tedsvoiceacademy.com/" },
        { "@type": "ListItem", "position": 2, "name": "PASS Profile", "item": "https://tedsvoiceacademy.com/pass-profile" }
      ]
    })} />
    <script type="application/ld+json" set:html={JSON.stringify({
      "@context": "https://schema.org",
      "@type": "FAQPage",
      "mainEntity": [
        {
          "@type": "Question",
          "name": "How is PASS Profile different from DISC or Myers-Briggs?",
          "acceptedAnswer": { "@type": "Answer", "text": "DISC, Myers-Briggs, and CliftonStrengths measure broad personality traits and sort you into types. PASS Profile focuses specifically on communication behaviors in professional settings. It presents 60 real workplace scenarios and measures 40 traits on a continuous 0-100 scale — creating more unique combinations than atoms in the observable universe." }
        },
        {
          "@type": "Question",
          "name": "Is PASS Profile a personality test?",
          "acceptedAnswer": { "@type": "Answer", "text": "No. PASS measures communication behaviors — how you actually communicate and interact in professional settings. It doesn't measure personality, intelligence, competence, or character. It's a professional development tool for self-awareness and intentional communication." }
        },
        {
          "@type": "Question",
          "name": "How does the PASS Profile assessment work?",
          "acceptedAnswer": { "@type": "Answer", "text": "You encounter 60 workplace scenarios. For each, you rate four possible responses on a 1-5 scale based on how much each sounds like you — producing 240 individual data points. Each response contributes to multiple traits simultaneously. There are no right or wrong answers. Your progress auto-saves on every response." }
        },
        {
          "@type": "Question",
          "name": "How long does the PASS Profile assessment take?",
          "acceptedAnswer": { "@type": "Answer", "text": "Most people complete it in 30 to 60 minutes. There's no time limit. Your progress auto-saves on every response, and you can pause and resume anytime." }
        },
        {
          "@type": "Question",
          "name": "Will my results match someone else's?",
          "acceptedAnswer": { "@type": "Answer", "text": "It's virtually impossible. PASS measures 40 traits on a continuous 0-100 scale, creating more unique combinations than atoms in the observable universe. Your communication fingerprint is mathematically unique to you." }
        },
        {
          "@type": "Question",
          "name": "What do the PASS Profile scores mean?",
          "acceptedAnswer": { "@type": "Answer", "text": "Each trait is scored 0-100 in three ranges: Natural (66-100) means this comes easily — it's your default. Flexible (36-65) means you adapt based on context. Intentional (0-35) means you can do this with conscious effort. All three ranges are equally valuable." }
        },
        {
          "@type": "Question",
          "name": "Can PASS Profile be used for hiring or screening?",
          "acceptedAnswer": { "@type": "Answer", "text": "No. PASS Profile is a professional development tool, not a hiring or screening instrument. It measures communication preferences, not competence, ability, or job performance." }
        },
        {
          "@type": "Question",
          "name": "Can I share my PASS Profile results with my team?",
          "acceptedAnswer": { "@type": "Answer", "text": "Yes. Your report includes a Working With Others section designed to be shared with colleagues, written in first person so teammates can quickly understand how to collaborate with you." }
        },
        {
          "@type": "Question",
          "name": "Can teams use PASS Profile together?",
          "acceptedAnswer": { "@type": "Answer", "text": "Yes. Each team member takes the individual PASS assessment first. Then the Team Dynamics Alignment report analyzes how everyone's communication patterns interact — revealing pair dynamics, six communication roles, collective strengths, and team-specific scenarios. TDA is available for teams of 2-12 people, priced from $39.99 to $119.99 by team size." }
        },
        {
          "@type": "Question",
          "name": "What is Voice-Brand Alignment?",
          "acceptedAnswer": { "@type": "Answer", "text": "Voice-Brand Alignment is the role-specific application layer on top of your PASS Profile. You provide your job title, industry, key audiences, and role description. It generates an 8-10 page report with a personal communication brand statement, role-specific strategic playbook, and 90-day integration plan. Available in the PASS + Voice-Brand Bundle for $59.99." }
        },
        {
          "@type": "Question",
          "name": "What is Team Dynamics Alignment?",
          "acceptedAnswer": { "@type": "Answer", "text": "Team Dynamics Alignment analyzes how a team's individual communication patterns interact. It reveals pair dynamics, communication roles (Initiator, Bridge, Anchor, Catalyst, Harmonizer, Strategist), collective strengths, and team-specific scenarios. Available for teams of 2-3 ($39.99), 4-8 ($79.99), or 9-12 ($119.99)." }
        },
        {
          "@type": "Question",
          "name": "What do I do with my results after I get them?",
          "acceptedAnswer": { "@type": "Answer", "text": "Your report includes a 90-day development roadmap with specific daily practices, a shareable Working With Others section for colleagues, a one-page Quick Reference Card, and a Similar Well-Known Communicator match. You can also book a coaching session to explore your results in depth." }
        },
        {
          "@type": "Question",
          "name": "How much does PASS Profile cost?",
          "acceptedAnswer": { "@type": "Answer", "text": "PASS Profile is $49.99. The PASS + Voice-Brand Bundle is $59.99 (saves $10). Team Dynamics Alignment is $39.99 for 2-3 members, $79.99 for 4-8, or $119.99 for 9-12. All reports are personalized PDFs delivered to your email within minutes." }
        }
      ]
    })} />
  </Fragment>

  <!-- Hero Section -->
  <section class="hero">
    <div class="container">
      <Breadcrumb items={[{ label: 'PASS Profile™' }]} schema={false} />
    </div>
    <div class="hero-content">
      <div class="hero-text">
        <div class="logo-card">
          <img src="/images/pass-profile-logo.webp" alt="PASS Profile - Personal Authentic Speaking Style" class="pass-logo" />
        </div>
        <h1>Discover Your Communication Fingerprint</h1>
        <p class="hero-subhead">Your communication style is as unique as your fingerprint. PASS Profile™ reveals how you naturally communicate in professional settings — helping you leverage your authentic strengths.</p>
        <div class="hero-cta">
          <a href="https://pass.tedsvoiceacademy.com" class="btn btn-primary">Get Your Profile — $49.99</a>
          <a href="https://pass.tedsvoiceacademy.com" class="btn btn-secondary">Get the Bundle — $59.99</a>
        </div>
        <p class="hero-bundle-note">Bundle includes Voice-Brand Alignment — your role-specific application report</p>
      </div>
      <div class="hero-visual">
        <img src="/images/pass-bubble.webp" alt="PASS Profile 3.0 - fingerprint inside speech bubble" class="hero-fingerprint" />
      </div>
    </div>
  </section>

  <SectionDivider variant="wave" topColor="var(--color-pass-teal)" bottomColor="var(--color-white)" />

  <!-- Answer Capsule — structured summary for AI search engines -->
  <section class="capsule-section">
    <div class="container">
      <p class="answer-capsule">PASS Profile is a professional communication assessment developed by Ted Chamberlain that measures 40 traits across six domains using 60 workplace scenarios. Unlike DISC (4 types) or Myers-Briggs (16 types), PASS scores each trait on a continuous 0-100 scale, producing a mathematically unique communication fingerprint. The assessment takes 30-60 minutes, costs $49.99, and delivers a personalized 35-48 page PDF report via email within minutes. A Voice-Brand Alignment bundle ($59.99) adds role-specific application. Team Dynamics Alignment ($39.99-$119.99) analyzes group communication for teams of 2-12. Available at pass.tedsvoiceacademy.com.</p>
      <p class="page-date">Last updated: <time datetime="2026-02-18">February 18, 2026</time></p>
    </div>
  </section>

  <!-- What It Measures -->
  <section class="measures">
    <div class="container">
      <h2 class="reveal">What PASS Profile™ Measures</h2>
      <p class="section-intro reveal">PASS Profile examines your communication patterns across six domains:</p>

      <div class="domains-grid reveal-stagger">
        <div class="domain-card reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><circle cx="12" cy="12" r="10"/><circle cx="12" cy="12" r="6"/><circle cx="12" cy="12" r="2"/></svg>Communication Purpose</h3>
          <p>What drives you when you communicate. Are you drawn to building relationships or achieving outcomes? Understanding others or influencing them?</p>
        </div>
        <div class="domain-card reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><path d="M12 5a3 3 0 1 0-5.997.125 4 4 0 0 0-2.526 5.77 4 4 0 0 0 .556 6.588A4 4 0 1 0 12 18Z"/><path d="M12 5a3 3 0 1 1 5.997.125 4 4 0 0 1 2.526 5.77 4 4 0 0 1-.556 6.588A4 4 0 1 1 12 18Z"/><path d="M15 13a4.5 4.5 0 0 1-3-4 4.5 4.5 0 0 1-3 4"/><path d="M17.599 6.5a3 3 0 0 0 .399-1.375"/><path d="M6.003 5.125A3 3 0 0 0 6.401 6.5"/><path d="M3.477 10.896a4 4 0 0 1 .585-.396"/><path d="M19.938 10.5a4 4 0 0 1 .585.396"/><path d="M6 18a4 4 0 0 1-1.967-.516"/><path d="M19.967 17.484A4 4 0 0 1 18 18"/></svg>Thinking Style</h3>
          <p>How you process and share ideas. Big-picture or detail-oriented? Linear or flexible? Analytical or intuitive?</p>
        </div>
        <div class="domain-card reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><path d="M16 21v-2a4 4 0 0 0-4-4H6a4 4 0 0 0-4 4v2"/><circle cx="9" cy="7" r="4"/><path d="M22 21v-2a4 4 0 0 0-3-3.87"/><path d="M16 3.13a4 4 0 0 1 0 7.75"/></svg>Relational Approach</h3>
          <p>How you navigate human dynamics. Direct or diplomatic? Task-focused or people-focused?</p>
        </div>
        <div class="domain-card reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><circle cx="12" cy="12" r="10"/><polyline points="12 6 12 12 16 14"/></svg>Temporal & Decisional Orientation</h3>
          <p>Your relationship with time and decisions. Future-focused or present-focused? Deliberate or quick?</p>
        </div>
        <div class="domain-card reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><path d="m12.83 2.18a2 2 0 0 0-1.66 0L2.6 6.08a1 1 0 0 0 0 1.83l8.58 3.91a2 2 0 0 0 1.66 0l8.58-3.9a1 1 0 0 0 0-1.83Z"/><path d="m22.54 12.43-1.42-.65-8.29 3.78a2 2 0 0 1-1.66 0l-8.29-3.78-1.42.65a1 1 0 0 0 0 1.84l8.58 3.91a2 2 0 0 0 1.66 0l8.58-3.9a1 1 0 0 0 .26-1.85Z"/><path d="m22.54 16.43-1.42-.65-8.29 3.78a2 2 0 0 1-1.66 0l-8.29-3.78-1.42.65a1 1 0 0 0 0 1.84l8.58 3.91a2 2 0 0 0 1.66 0l8.58-3.9a1 1 0 0 0 .26-1.85Z"/></svg>Social Positioning</h3>
          <p>Your natural role in groups. Leading or supporting? Visible or behind-the-scenes? Collaborative or independent?</p>
        </div>
        <div class="domain-card reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><path d="M21 12a9 9 0 1 1-9-9c2.52 0 4.93 1 6.74 2.74L21 8"/><path d="M21 3v5h-5"/></svg>Adaptability</h3>
          <p>How consistently or flexibly you communicate across situations.</p>
        </div>
      </div>
      
      <p class="section-closing">Within these six domains, PASS measures 40 specific traits. Together, they create your unique communication fingerprint.</p>
    </div>
  </section>

  <!-- Understanding Your Scores -->
  <section class="scores">
    <div class="container">
      <h2 class="reveal">Understanding Your Scores</h2>
      <p class="section-intro reveal">Each of your 40 traits is measured on a scale from 0 to 100. These scores represent where you naturally fall on a continuum of communication tendencies.</p>

      <div class="score-ranges reveal-stagger">
        <div class="score-range high reveal">
          <div class="score-label">66–100</div>
          <div class="score-name">Natural</div>
          <div class="score-description">
            This comes easily to you. It's your default setting — where you go without thinking about it.
          </div>
        </div>
        <div class="score-range mid reveal">
          <div class="score-label">36–65</div>
          <div class="score-name">Flexible</div>
          <div class="score-description">
            You adapt based on the situation. You can go either way depending on context, giving you natural versatility.
          </div>
        </div>
        <div class="score-range low reveal">
          <div class="score-label">0–35</div>
          <div class="score-name">Intentional</div>
          <div class="score-description">
            You can absolutely do this — it takes conscious effort. This is a strategic choice you make when the situation calls for it.
          </div>
        </div>
      </div>

      <div class="key-insight reveal">
        <p><strong>All three ranges are equally valuable.</strong> A Natural score means a behavior flows automatically. An Intentional score means you have conscious control over when and how you deploy it. Neither is better or worse — they are different strategic positions.</p>
        <p>Your report analyzes all 40 trait scores together — how they combine and interact is what makes your pattern genuinely yours.</p>
      </div>
    </div>
  </section>

  <SectionDivider variant="curve" topColor="var(--color-white)" bottomColor="var(--color-pass-teal)" />

  <!-- How It's Different -->
  <section class="different">
    <div class="container">
      <h2 class="reveal">How PASS Profile™ Is Different</h2>
      <p class="section-intro reveal">You may have taken assessments like DISC, Myers-Briggs, or CliftonStrengths. They measure broad personality traits and sort you into types. PASS Profile offers something different.</p>

      <div class="differentiators reveal-stagger">
        <div class="diff-item reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><path d="M12 2a3 3 0 0 0-3 3v7a3 3 0 0 0 6 0V5a3 3 0 0 0-3-3Z"/><path d="M19 10v2a7 7 0 0 1-14 0v-2"/><line x1="12" x2="12" y1="19" y2="22"/></svg>Communication-specific</h3>
          <p>PASS focuses on how you communicate and interact in professional settings — the behaviors that shape your daily work relationships and effectiveness.</p>
        </div>
        <div class="diff-item reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><polygon points="13 2 3 14 12 14 11 22 21 10 12 10 13 2"/></svg>Scenario-based</h3>
          <p>PASS presents real workplace situations and asks what you actually do. This captures your authentic patterns in action.</p>
        </div>
        <div class="diff-item reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><path d="M12 10a2 2 0 0 0-2 2c0 1.02-.1 2.51-.26 4"/><path d="M14 13.12c0 2.38 0 6.38-1 8.88"/><path d="M17.29 21.02c.12-.6.43-2.3.5-3.02"/><path d="M2 12a10 10 0 0 1 18-6"/><path d="M2 16h.01"/><path d="M21.8 16c.2-2 .131-5.354 0-6"/><path d="M5 19.5C5.5 18 6 15 6 12a6 6 0 0 1 .34-2"/><path d="M8.65 22c.21-.66.45-1.32.57-2"/><path d="M9 6.8a6 6 0 0 1 9 5.2c0 .47 0 1.17-.02 2"/></svg>Truly individual</h3>
          <p>Most assessments sort you into one of 4, 16, or 34 types. PASS generates more unique combinations than atoms in the observable universe — making it virtually impossible for two people to share the same exact profile.</p>
        </div>
        <div class="diff-item reveal">
          <h3><svg class="icon-inline" width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" aria-hidden="true"><path d="M15 14c.2-1 .7-1.7 1.5-2.5 1-.9 1.5-2.2 1.5-3.5A6 6 0 0 0 6 8c0 1 .2 2.2 1.5 3.5.7.7 1.3 1.5 1.5 2.5"/><path d="M9 18h6"/><path d="M10 22h4"/></svg>100% asset-based</h3>
          <p>Every trait represents a strength in certain contexts. Your unique pattern shows where you naturally excel and how to leverage your tendencies with intention.</p>
        </div>
      </div>
    </div>
  </section>

  <SectionDivider variant="wave" topColor="var(--color-pass-teal)" bottomColor="var(--color-cream)" />

  <!-- What You'll Receive -->
  <section class="receive">
    <div class="container">
      <h2 class="reveal">What You'll Receive</h2>
      <p class="section-intro reveal">After completing the assessment (30–60 minutes), you'll receive a comprehensive PDF report — typically 35-48 pages — delivered to your email.</p>
      <p class="reveal">Your report draws on all 40 of your trait scores to create a complete picture of your communication style. Every insight reflects how your full pattern works together — peaks and valleys alike.</p>

      <div class="report-contents">
        <h3>Your report includes:</h3>
        <div class="contents-grid reveal-stagger">
          <div class="content-item reveal">
            <h4>Your Communication Style Title</h4>
            <p>A personalized title that captures your distinctive pattern (like "Context-Aware Communicator" or "Strategic Relationship Builder").</p>
          </div>
          <div class="content-item reveal">
            <h4>Communication Fingerprint</h4>
            <p>A radar chart visualization of your six domain scores, showing your unique pattern at a glance.</p>
          </div>
          <div class="content-item reveal">
            <h4>Top 10 Natural Strengths</h4>
            <p>Your highest-scoring traits, ranked and explained — with context for how they interact with your complete pattern.</p>
          </div>
          <div class="content-item reveal">
            <h4>Score Distribution & Balance Wheel</h4>
            <p>Visual representations of how your 40 traits distribute across the spectrum.</p>
          </div>
          <div class="content-item reveal">
            <h4>Six Domain Deep-Dives</h4>
            <p>Detailed analysis of each domain with personalized insights and practical applications (~2 pages each).</p>
          </div>
          <div class="content-item reveal">
            <h4>Situational Effectiveness Map</h4>
            <p>Where your style naturally excels, and contexts where you might consciously expand your range.</p>
          </div>
          <div class="content-item reveal">
            <h4>Career Contexts</h4>
            <p>How your pattern shows up across 15 different professional contexts, with specific insights for each.</p>
          </div>
          <div class="content-item reveal">
            <h4>Practical Scenarios</h4>
            <p>Six common workplace situations with personalized guidance based on your pattern.</p>
          </div>
          <div class="content-item reveal">
            <h4>Similar Well-Known Communicator</h4>
            <p>A public figure who shares your communication patterns, with insights on what you can learn from their approach.</p>
          </div>
          <div class="content-item reveal">
            <h4>Working With Others</h4>
            <p>A shareable section designed for colleagues, helping them understand how to communicate effectively with you.</p>
          </div>
          <div class="content-item reveal">
            <h4>90-Day Development Roadmap</h4>
            <p>Practical steps for leveraging your strengths and expanding your range.</p>
          </div>
          <div class="content-item reveal">
            <h4>Quick Reference Card</h4>
            <p>A one-page summary for daily use.</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Who It's For -->
  <section class="who">
    <div class="container">
      <h2 class="reveal">Who PASS Profile™ Is For</h2>
      <p class="section-intro reveal">Communication is at the heart of all human interaction. How you communicate shapes your relationships, your effectiveness, and your experience at work. Understanding your natural patterns — and learning to leverage them — has profound implications.</p>

      <div class="audience-grid reveal-stagger">
        <div class="audience-item reveal">
          <h3>Professionals at any stage</h3>
          <p>Who want to communicate with more clarity, confidence, and intention. When you understand your style, you experience greater job satisfaction, fewer workplace conflicts, improved productivity, and stronger professional relationships.</p>
        </div>
        <div class="audience-item reveal">
          <h3>Leaders and managers</h3>
          <p>Who want to understand how their communication style impacts their teams — and how to adapt when the situation calls for it. Pair PASS with <a href="/speaking/">speaking coaching</a> for maximum impact.</p>
        </div>
        <div class="audience-item reveal">
          <h3>People in transition</h3>
          <p>New role, promotion, career change, or team reorganization — who want to communicate with confidence in a new context.</p>
        </div>
        <div class="audience-item reveal">
          <h3>Teams and organizations</h3>
          <p>Seeking better collaboration. When team members understand each other's communication patterns, they work together more effectively — leading to improved performance and stronger outcomes.</p>
        </div>
        <div class="audience-item reveal">
          <h3>Coaches, consultants, and HR professionals</h3>
